    <ApplicationIcon>Resources\calculator.ico</ApplicationIcon>
    <GenerateDocumentationFile>True</GenerateDocumentationFile>
  </PropertyGroup>

  <ItemGroup>
    <Content Include="Resources\calculator.ico" />
